#footer
  clearfix()
  font-size: 0.9em
  color: color-minor
  line-height: app-footer-height
  position: absolute
  bottom: 40px
  left: 50%
  width: app-page-width
  margin-left: @width * -0.5

#footer-links
  float: left

#footer-logo
  float: right
  color: #d6d6d6
  font-family: font-title
  font-size: app-footer-height
  font-weight: 600
  text-shadow: 0 1px #fff